<p id="notice"><%= notice %></p>

<h1>Slot Types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @slot_types.each do |slot_type| %>
      <tr>
        <td><%= link_to 'Show', slot_type %></td>
        <td><%= link_to 'Edit', edit_slot_type_path(slot_type) %></td>
        <td><%= link_to 'Destroy', slot_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Slot Type', new_slot_type_path %>
