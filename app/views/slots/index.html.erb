



<%= render 'newsletters/newsletter_information', newsletter: @newsletter %>
<% if current_user_owns_slot? @slots.first %>
  <%= link_to '> New Slot <', new_newsletter_slot_path(@newsletter, @slot), class: 'm-2 p-2 font-semibold' %>
<% end %>

<%= render 'slots_list', :locals => { :future_slots => @future_slots, :past_slots => @past_slots } %>


<button id="button__share-link" type="button" class="inline-flex items-center px-4 py-2 border border-transparent shadow-sm text-sm font-medium rounded-md text-white bg-green-600 hover:bg-green-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-green-500">
  Share a link to this page
  <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24" stroke="currentColor">
    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8.684 13.342C8.886 12.938 9 12.482 9 12c0-.482-.114-.938-.316-1.342m0 2.684a3 3 0 110-2.684m0 2.684l6.632 3.316m-6.632-6l6.632-3.316m0 0a3 3 0 105.367-2.684 3 3 0 00-5.367 2.684zm0 9.316a3 3 0 105.368 2.684 3 3 0 00-5.368-2.684z" />
  </svg>
</button>
<%= javascript_pack_tag 'slots.js' %>
